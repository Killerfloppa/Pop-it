local Library = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()

local Window = Library.CreateLib("Pop it trading private", "Ocean")

local AutoTab = Window:NewTab("Buy robux item auto")

local AutoSection = AutoTab:NewSection("Mario items")

AutoSection:NewToggle("Helloshi", "ToggleInfo", function(state)

    if state then _G.on = true while _G.on do wait() local args = { [1] = "Helloshi" } 

game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args)) end else _G.on = false end end)

AutoSection:NewToggle("Omb Bomb", "ToggleInfo", function(state)

    if state then _G.on = true while _G.on do wait() local args = { [1] = "Omb Bomb" } 

game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args)) end else _G.on = false end end)

AutoSection:NewToggle("Markio", "ToggleInfo", function(state)

    if state then _G.on = true while _G.on do wait() local args = { [1] = "Markio" } 

game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args)) end else _G.on = false end end)

AutoSection:NewToggle("Rainbow Star", "ToggleInfo", function(state)

    if state then _G.on = true while _G.on do wait() local args = { [1] = "Rainbow Star" } 

game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args)) end else _G.on = false end end)

AutoSection:NewToggle("Gold Coin", "ToggleInfo", function(state)

    if state then _G.on = true while _G.on do wait() local args = { [1] = "Gold Coin" } 

game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args)) end else _G.on = false end end)

AutoSection:NewToggle("Luisi", "ToggleInfo", function(state)

    if state then _G.on = true while _G.on do wait() local args = { [1] = "Luisi" } 

game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args)) end else _G.on = false end end)

local BuyTab = Window:NewTab("Buy items robux")

local BuySection = BuyTab:NewSection("Mario items")

BuySection:NewButton("Helloshi", "ButtonInfo", function()

    local args = {

    [1] = "Helloshi"

}

game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args))

end)

BuySection:NewButton("Omb Bomb", "ButtonInfo", function()

    local args = {

    [1] = "Omb Bomb"

}

game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args))

